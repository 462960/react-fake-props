{
  "name": "react-fake-props",
  "version": "0.1.2",
  "description": "Automatically generate fake props for your React tests",
  "keywords": [
    "react",
    "enzyme",
    "test",
    "fake",
    "props",
    "flow",
    "proptypes",
    "react-docgen"
  ],
  "main": "lib/index.js",
  "repository": "https://github.com/typicode/react-fake-props",
  "author": "typicode <typicode@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "standard && jest",
    "build": "rimraf lib && babel src --out-dir lib",
    "fix": "prettier **/*.js --write && standard --fix",
    "prepublishOnly": "npm run build && pkg-ok",
    "precommit": "npm test"
  },
  "dependencies": {
    "react-docgen": "^2.20.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "pkg-ok": "^1.1.0",
    "prettier": "^1.8.2",
    "react": "^16.1.1",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "env": "jest"
  }
}
